<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="Ebay Test Strips" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="80.0"/>
    <parameter name="url" value="http://ebay.com"></parameter>

    <test name = "Ebay Home">

        <classes>
            <class name="search.EbaySearch"></class>
        </classes>

    </test>

</suite>